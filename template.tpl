const sendHttpRequest = require('sendHttpRequest');
const getTimestampMillis = require('getTimestampMillis');
const encodeUriComponent = require('encodeUriComponent');
const JSON = require('JSON');
const getRemoteAddress = require('getRemoteAddress');
const getContainerVersion = require('getContainerVersion');
const logToConsole = require('logToConsole');
const getRequestHeader = require('getRequestHeader');
const isLoggingEnabled = determinateIsLoggingEnabled();
const traceId = isLoggingEnabled ? getRequestHeader('trace-id') : undefined;

const klaviyoApiRevision = '2024-10-15';
subscriptionToList();

function determinateIsLoggingEnabled() {
  const containerVersion = getContainerVersion();
  const isDebug = !!(
    containerVersion &&
    (containerVersion.debugMode || containerVersion.previewMode)
  );

  if (!data.logType) {
    return isDebug;
  }

  if (data.logType === 'no') {
    return false;
  }

  if (data.logType === 'debug') {
    return isDebug;
  }

  return data.logType === 'always';
}

function subscriptionToList() {
  let url = 'https://a.klaviyo.com/client/subscriptions?company_id=' + encodeUriComponent(data.publicApiKey);

  let profileProperties = {
      data: {
        type: 'subscription',
        attributes: {
          profile: {
          data: 
            {
             type: 'profile',
             attributes: {
             properties: {}
             },  
            }
          }
        },
      relationships: {
        list: {
          data: {
            type: 'list',
            id: data.listId
          }
        }
      }        
      }
  };
  let profileStandardProperties = profileProperties.data.attributes.profile.data.attributes;
  let profileCustomProperties = profileProperties.data.attributes.profile.data.attributes.properties;
  
  if (data.profileStandardProperties) {
    for (let keyIndex in data.profileStandardProperties) {
      profileStandardProperties[data.profileStandardProperties[keyIndex].name] =
        data.profileStandardProperties[keyIndex].value;
    }
  }

  if (data.profileLocationProperties) {
    profileStandardProperties.location = {};

    for (let keyIndex in data.profileLocationProperties) {
      profileStandardProperties.location[data.profileLocationProperties[keyIndex].name] =
        data.profileLocationProperties[keyIndex].value;
    }
  }

  if (data.profileProperties) {
    for (let keyIndex in data.profileProperties) {
      profileCustomProperties[data.profileProperties[keyIndex].name] =
        data.profileProperties[keyIndex].value;
    }
